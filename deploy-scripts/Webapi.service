[Unit]
decription=.Net Reporting Tool App

[Service]
WorkingDirectory=/var/www/
ExecStart= /usr/bin/dotnet /var/www/ReportingToolMVC.API.dll
RestartSec=10
KillSignal=SIGINT
SyslogIdentifier=webapi
Enviroment=ASPNETCORE_ENVIROMENT=Production
Enviroment=DOTNET_PRINTTELEMETRY_MESSAGE=false
Enviroment=ASPNETCORE_URL=http://*:80

[Install]
WantedBy=multi-user.target
